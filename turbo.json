{
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ],
      "cache": false
    },
    "test": {
      "dependsOn": [
        "^test-typescript",
        "^lint",
        "^build"
      ]
    },
    "clean": {
      "outputs": [
        "dist/**"
      ],
      "cache": false
    },
    "test-typescript": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "lint-fix": {
      "cache": false
    },
    "dev": {
      "cache": false
    }
  }
}
