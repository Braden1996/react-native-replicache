{
  "expo": {
    "name": "React Native Replicache",
    "slug": "react-native-replicache",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.braden1996.rnreplicache"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.braden1996.rnreplicache"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "flipper": "0.174.0"
          },
          "ios": {
            "flipper": "0.174.0"
          }
        }
      ]
    ]
  }
}
